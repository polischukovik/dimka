  <connectionStrings>
    <add name="WindowsFormsApplication2.Properties.Settings.DBConnectionString"
      connectionString="Data Source=|DataDirectory|\DB.sdf" providerName="Microsoft.SqlServerCe.Client.4.0" />
  </connectionStrings>
