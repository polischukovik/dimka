      <setting name="MapImagesFileTypes" serializeAs="String">
        <value>.jpg,.tif</value>
      </setting>
      <setting name="SQLiteCreateStatement" serializeAs="String">
        <value>CREATE TABLE [FieldImage] (
                    [id] integer PRIMARY KEY AUTOINCREMENT NOT NULL,
                    [name] char(100) NOT NULL,
                    [image_data] BLOB,
                    [image_coord] BLOB,
                    [timeadded] char(100) 
                    );</value>
      </setting>
    </DSS.Properties.Settings>
  </userSettings>
